{
  "name": "@jemurai/nestjs-stripe",
  "version": "2.0.0",
  "description": "Provides an injectable Stripe client to nestjs modules",
  "repository": "https://github.com/CruxSecurity/nestjs-stripe",
  "author": {
    "name": "Dylan Aspden",
    "url": "http://github.com/dhaspden"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "codecov": "codecov",
    "format": "prettier --write '**/*.md' '**/*.json' '**/*.ts' '**/*.yml' .prettierrc",
    "prebuild": "rimraf lib",
    "prepublish:npm": "yarn build",
    "publish:npm": "yarn publish --access public",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "@nestjs/testing": "^11.0.11",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.1",
    "codecov": "3.8.3",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "stripe": "^17.7.0",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "@nestjs/common": ">=^6.0.0 < 12.*",
    "@nestjs/core": ">=^6.0.0 < 12.*",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0 || ^7.1.0",
    "stripe": "^17.7.0"
  },
  "keywords": [
    "nestjs",
    "stripe",
    "payment"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
