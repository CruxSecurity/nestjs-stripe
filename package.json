{
  "name": "@jemurai/nestjs-stripe",
  "version": "1.1.0",
  "description": "Provides an injectable Stripe client to nestjs modules",
  "repository": "https://github.com/dhaspden/nestjs-stripe",
  "author": {
    "name": "Dylan Aspden",
    "url": "https://github.com/dhaspden"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "codecov": "codecov",
    "format": "prettier --write '**/*.md' '**/*.json' '**/*.ts' '**/*.yml' .prettierrc",
    "prebuild": "rimraf lib",
    "prepublish:npm": "yarn build",
    "publish:npm": "yarn publish --access public",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@nestjs/common": "^9.4.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/testing": "^9.4.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "codecov": "^3.8.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "stripe": "^12.8.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.5",
    "@nestjs/core": "^9.0.5",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^7.1.0",
    "stripe": "^12.0.0"
  },
  "keywords": [
    "nestjs",
    "stripe",
    "payment"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {}
}
